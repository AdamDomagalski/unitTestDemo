{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"

  },
  "rules": {
    //    full list can be found: https://eslint.org/docs/rules/
    "indent": [
      "error",
      4,
      { "SwitchCase": 1 }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "import/no-commonjs": [
      "off"
    ],
    "import/no-nodejs-modules": [
      "off"
    ],
    "curly": [
      "error",
      "all"
    ],
    "operator-linebreak": [
      "error",
      "after"
    ],
    "arrow-parens": [
      "error",
      "always"
    ],
    "no-sync": [
      "off"
    ],
    "arrow-body-style": [
      "error"
    ],
    "class-methods-use-this": [
      "error"
    ],
    "dot-notation": [
      "error"
    ],
    "eqeqeq": [
      "error"
    ],
    "import/no-amd": [
      "error"
    ],
    "import/no-duplicates": [
      "error"
    ],
    "import/no-mutable-exports": [
      "error"
    ],
    "import/no-namespace": [
      "error"
    ],
    "import/prefer-default-export": [
      "error"
    ],
    "import/no-extraneous-dependencies": [
      "off"
    ],
    "no-alert": [
      "error"
    ],
    "no-duplicate-imports": [
      "error"
    ],
    "no-empty-function": [
      "error"
    ],
    "no-else-return": [
      "error"
    ],
    "no-eval": [
      "error"
    ],
    "no-extend-native": [
      "error"
    ],
    "no-extra-bind": [
      "error"
    ],
    "no-global-assign": [
      "error"
    ],
    "no-implicit-globals": [
      "error"
    ],
    "no-implied-eval": [
      "error"
    ],
    "no-invalid-this": [
      "error"
    ],
    "no-lonely-if": [
      "error"
    ],
    "no-loop-func": [
      "error"
    ],
    "no-new": [
      "error"
    ],
    "no-new-func": [
      "error"
    ],
    "no-new-wrappers": [
      "error"
    ],
    "no-proto": [
      "error"
    ],
    "no-script-url": [
      "error"
    ],
    "no-self-compare": [
      "error"
    ],
    "no-throw-literal": [
      "error"
    ],
    "no-unmodified-loop-condition": [
      "error"
    ],
    "no-unsafe-negation": [
      "error"
    ],
    "no-useless-call": [
      "error"
    ],
    "no-useless-computed-key": [
      "error"
    ],
    "no-useless-concat": [
      "error"
    ],
    "no-useless-constructor": [
      "error"
    ],
    "no-useless-escape": [
      "error"
    ],
    "no-useless-rename": [
      "error"
    ],
    "no-var": [
      "error"
    ],
    "no-with": [
      "error"
    ],
    "object-shorthand": [
      "error"
    ],
    "operator-assignment": [
      "error"
    ],
    "prefer-arrow-callback": [
      "error"
    ],
    "prefer-const": [
      "error"
    ],
    "prefer-rest-params": [
      "error"
    ],
    "prefer-spread": [
      "error"
    ],
    "array-callback-return": [
      "error"
    ],
    "no-constant-condition": [
      "error",
      {
        "checkLoops": false
      }
    ],
    "no-unneeded-ternary": [
      "error",
      {
        "defaultAssignment": false
      }
    ],
    "no-unused-expressions": [
      "error",
      {
        "allowTernary": true,
        "allowShortCircuit": true
      }
    ],
    "no-use-before-define": [
      "error",
      "nofunc"
    ],
    "complexity": ["warn",4],
    "max-depth": ["warn",4],
    "max-statements": ["warn",10],
    "max-params": ["warn",4],
    "max-nested-callbacks": ["warn",5],
    "max-lines": ["warn", {"max": 500, "skipBlankLines": true,"skipComments": true}],
    "max-len": ["warn",{"code": 140,"tabWidth": 4,"ignoreComments": true,"ignoreUrls": true}],
    "no-cond-assign": "error",
    "no-console": "error",
    "no-dupe-args": "error",
    "no-dupe-keys":"error",
    "no-duplicate-case": "error",
    "no-empty": "error",
    "no-ex-assign": "error",
    "no-extra-boolean-cast": "warn",
    "no-extra-parens": "warn",//error
    "no-extra-semi": "error",
    "no-func-assign": "error",
    "no-irregular-whitespace": "error",
    "no-obj-calls": "error",
    "no-sparse-arrays": "error",
    "no-template-curly-in-string": "warn",
    "no-unexpected-multiline": "error",
    "no-unreachable": "error",
    "no-unsafe-finally" : "error",
    "require-atomic-updates": "warn",//error
    "use-isnan": "error",
    "valid-typeof": "error",
    "consistent-return": "warn",//error
    "default-case": "warn",//error
    "dot-location": ["error", "property"],
    "guard-for-in": "warn",//error
    "max-classes-per-file": ["error",1],
    "no-caller": "error",
    "no-case-declarations": "error",
    "no-div-regex": "error",
    "no-empty-pattern": "error",
    "no-eq-null": "error",
    "no-extra-label": "error",
    "no-fallthrough": "error",
    "no-floating-decimal": "error",
    "no-iterator": "error",
    "no-labels": "error",
    "no-lone-blocks": "error",
    "no-magic-numbers": ["warn",{"ignore": [0,1]}], //error
    "no-multi-spaces": "warn",//error
    "no-multi-str": "error",
    "no-octal":"error",
    "no-octal-escape": "error",
    "no-param-reassign": "warn",//error
    "no-redeclare": "error",
    "no-restricted-properties": "error",
    "no-return-assign": "warn",//error
    "no-return-await": "warn", //error
    "no-self-assign": "error",
    "no-sequences": "error",
    "no-unused-labels": "error",
    "no-useless-catch": "error",
    "no-useless-return": "warn",//error
    "no-void": "error",
    "no-warning-comments": "warn",
    "prefer-promise-reject-errors": "error",
    "radix": "off",//error
    "require-await": "warn",//error
    "wrap-iife": "warn",//error
    "yoda": "error",
    "no-delete-var": "error",
    "no-label-var": "error",
    "no-shadow": "warn",//error
    "no-shadow-restricted-names": "error",
    "no-undef": "error",
    "no-undef-init": "error",
    "no-undefined": "off",
    "no-unused-vars": "error",
    "callback-return": "warn",
    "global-require": "warn",//error
    "handle-callback-err": "warn",//error
    "no-buffer-constructor": "error",
    "no-mixed-requires": "error",
    "no-new-require": "error",
    "no-path-concat": "error",
    "no-process-env": "off",
    "no-process-exit": "warn",
    "array-bracket-newline": "off",
    "array-bracket-spacing": "off",//error
    "array-element-newline": "off",
    "block-spacing": "warn",//error
    "brace-style": "off",//error
    "camelcase": "warn",//error
    "comma-dangle": "warn",//error
    "comma-spacing": ["warn", { "before": false, "after": true }],//error
    "comma-style": ["warn", "last"], //error
    "computed-property-spacing": "warn",//error
    "consistent-this": "warn", //error
    "eol-last": "off",
    "func-call-spacing": ["warn","never"],//error
    "func-name-matching": "error",
    "func-names": "off",
    "func-style": ["warn","declaration"],
    "id-length": "off",
    "id-match": "off",
    "implicit-arrow-linebreak": "off",//["warn", "beside"], //error
    "jsx-quotes": ["error", "prefer-single"],
    "key-spacing": ["warn",{//error
      "beforeColon": false,
      "afterColon": true
    }],
    "keyword-spacing": "off",
    "line-comment-position": "off",
    "lines-around-comment": "off",
    "lines-between-class-members": "off",
    "max-lines-per-function": ["warn",{//error
      "max": 80,
      "skipBlankLines": true,
      "skipComments": true
    }],
    "max-statements-per-line": ["error",{
      "max": 2
    }],
    "multiline-comment-style": "off",
    "multiline-ternary": "off",
    "new-cap": "off",//error
    "new-parens": "error",
    "newline-per-chained-call": "off",
    "no-array-constructor": "off",
    "no-bitwise": "error",
    "no-continue": "error",
    "no-inline-comments": "off",
    "no-mixed-operators": "off",
    "no-mixed-spaces-and-tabs": "error",
    "no-multi-assign": "warn",//error
    "no-multiple-empty-lines": "error",
    "no-negated-condition": "off",
    "no-nested-ternary": "off",
    "no-new-object": "off",
    "no-plusplus": "off",
    "no-tabs": "error",
    "no-ternary": "off",
    "no-trailing-spaces": "error",
    "no-whitespace-before-property": "error",
    "object-curly-newline": "off",
    "object-curly-spacing": "off",
    "object-property-newline" : [ "error", { "allowAllPropertiesOnSameLine": true }],
    "one-var": "off",
    "one-var-declaration-per-line": "error",
    "padded-blocks": "off",
    "padding-line-between-statements": "off",
    "prefer-object-spread": "error",
    "quote-props": ["error", "as-needed"],
    "semi-spacing": ["error", { "before": false, "after": false }],
    "semi-style": ["error", "last"],
    "sort-keys": "off",
    "sort-vars": "off",
    "constructor-super": "error",
    "no-class-assign": "error",
    "no-confusing-arrow": "off",
    "no-const-assign": "error",
    "no-dupe-class-members":"error",
    "no-this-before-super": "error",
    "prefer-numeric-literals": "error",
    "prefer-template":"error"

  }}